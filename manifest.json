{
  "manifest_version": 2,

  "name": "Like me later",
  "description": "This extension will like facebook posts for you at a later time",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
